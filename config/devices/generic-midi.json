{
  "device": {
    "name": "Generic MIDI Controller",
    "protocol": "midi",
    "vendor": "*",
    "product": "*"
  },
  "mappings": {
    "play_a": {
      "midi": {
        "type": "noteon",
        "channel": 0,
        "note": 11
      },
      "action": {
        "type": "transport",
        "command": "play",
        "deck": "A"
      },
      "target": "audio",
      "priority": "high",
      "feedback": {
        "type": "led",
        "midiOut": {
          "type": "noteon",
          "channel": 0,
          "note": 11
        },
        "stateMap": {
          "playing": 127,
          "stopped": 0
        }
      }
    },
    "play_b": {
