      "eq_hi_b": {
        "bytes": [26, 27],
        "type": "absolute",
        "resolution": 14,
        "min": 0,
        "max": 16383
      },

      "eq_mid_b": {
        "bytes": [28, 29],
        "type": "absolute",
        "resolution": 14,
        "min": 0,
        "max": 16383
      },

      "eq_low_b": {
        "bytes": [30, 31],
        "type": "absolute",
        "resolution": 14,
        "min": 0,
        "max": 16383
